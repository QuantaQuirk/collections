{
    "name": "quantaquirk/collections",
    "description": "The QuantaQuirk Collections package.",
        "license": "proprietary",
    "authors": [
        {
            "name": "Tomáš Selič (Smisch-DEV)",
            "email": "contact@smisch.sk"
        }
    ],
    "require": {
        "php": "^8.1",
        "quantaquirk/conditionable": "^10.0",
        "quantaquirk/contracts": "^10.0",
        "quantaquirk/macroable": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "QuantaQuirk\\Support\\": ""
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        }
    },
    "suggest": {
        "symfony/var-dumper": "Required to use the dump method (^6.2)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
